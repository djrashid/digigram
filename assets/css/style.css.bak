/*border property*/
.b{
	border:2px #EFEEF3 solid;
}
.b-shadow{
	box-shadow: 3px 6px;
}
.b-0{
	border:0px;
}
.br-0{
	border-right:0px;
}
.b-1{
	border:1px #8000FF solid;
}
.b-2{
	border:20px;
}
.bt-1{
	border-top:1px #FAFAFB solid; 
}
.blr-1{
	border:0px 10px;
}
.bb-1{
	border-bottom:2px #EFEEF3 solid;
}
.b-rad{
	border-radius: 30px;
}
.b-rad-60{
	border-radius: 60px;
}

