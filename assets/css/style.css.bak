*{
box-sizing:border-box;
}
/*container property*/
.container-full{
	max-width:100%;
	height: auto;
	margin: 0px auto;
}
.container{
	max-width:70%;
	height: auto;
	margin: 0px auto;
}
.container-80{
	max-width:80%;
	margin: 0px auto;
}
.box{
	width:auto;
	height:auto;
	margin: 0px auto;
	align-items: center;
}

/*width-height property*/
.w-auto{
	width:auto;
}
.w-33{
	width:33%;
}
.w-23{
